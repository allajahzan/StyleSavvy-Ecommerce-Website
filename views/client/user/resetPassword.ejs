<!DOCTYPE html>
<html lang="en">


<!-- molla/login.html  22 Nov 2019 10:04:03 GMT -->

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Reset Password</title>
    <meta name="keywords" content="StyleSavvy">
    <meta name="description" content="StyleSavvy">
    <meta name="author" content="Ahsanallajpk">
    <!-- Favicon -->
    <link rel="shortcut icon" href="/assets/images/demos/demo-5/ss.png">

    <!-- google icons -->
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,400,0,0" />


    <!-- Plugins CSS File -->
    <link rel="stylesheet" href="/assets/css/bootstrap.min.css">
    <!-- Main CSS File -->
    <link rel="stylesheet" href="/assets/css/demos/demo-5.css">
    <link rel="stylesheet" href="/assets/css/style.css">
    <link rel="stylesheet" href="/assets/css/myStyle.css">

</head>

<body>

    <!-- loading -->
    <section id="loadings">
        <h2 id="footer-logo">StyleSavvy</h2>
        <span class="loader"></span>
    </section>

    
    <div id="snackbar">
        <span style="color: green;" id="snackbar_icon" class="material-symbols-outlined">
            task_alt
        </span>
        <p id="snackbar_msg"></p>
    </div>


    <section style="display: block;" id="verify_page">
        <header style="padding: 4px 0px 8px 0px;" class="header header-5">
            <div class="header-middle sticky-header">
                <div class="container">
                    <div class="header-left">
                        <h2 id="logo">StyleSavvy</h2>
                    </div>
                </div>
            </div>
        </header>

        <main class="main">
            <div class="login-page verify_page">
                <div class="container cont">
                    <!-- <p class="p_heading" style="text-align: center;"><br><span id="verify_email"></span></p> -->
                    <div style="position: relative; top: -10px;" class="form-box">
                        <div class="form-tab">
                            <ul class="nav nav-pills nav-fill" role="tablist">
                                <li class="nav-item">
                                    <a style="border-width: 0px 0px 2px 0px; border-color: #bf8040; border-style: solid;"
                                        class="nav-link active">Reset Password</a>
                                </li>
                            </ul>
                            <div class="tab-content">

                                <div class="tab-pane fade show active" id="register-2" role="tabpanel"
                                    aria-labelledby="register-tab-2">

                                    <form id="password_form" onsubmit="saveNewPassword(event)">
                                        <div class="form-group">
                                            <label for="singin-password-2">NewPassword *</label>
                                            <div style="padding-bottom: 5px;" id="field_div">
                                                <input type="password" class="form-control" id="password1"
                                                    name="password" required>
                                                <span id="password_error1" class="material-symbols-outlined">

                                                </span>
                                                <span onclick="showPassword1()" id="password_visibility1"
                                                    class="material-symbols-outlined pass_icon">
                                                    visibility_off
                                                </span>
                                            </div>
                                            <label for="singin-password-2">Confirm Password *</label>
                                            <div id="field_div">
                                                <input type="password" class="form-control" id="password2"
                                                    name="password" required>
                                                <span id="password_error2" class="material-symbols-outlined">

                                                </span>
                                                <span onclick="showPassword2()" id="password_visibility2"
                                                    class="material-symbols-outlined pass_icon">
                                                    visibility_off
                                                </span>
                                            </div>
                                        </div>
                                        <div dis class="form-footer">
                                            <button id="reset_btn" type="submit" class="btn btn-outline-primary-2">
                                                <span>Reset Password</span>
                                                <i class="icon-long-arrow-right"></i>
                                            </button>
                                            <!-- <div id="otp_div">
                                            <p id="otp_p">Resend OTP in <span id="timer_otp"></span> seconds</p>
                                            <a id="otp_button">Resend OTP?</a>
                                        </div> -->
                                        </div><!-- End .form-group -->
                                    </form>
                                    <!-- End .form-choice -->
                                </div><!-- .End .tab-pane -->
                            </div><!-- End .tab-content -->
                        </div><!-- End .form-tab -->
                        <div id="toast-div">
                            <h6>Email Verified Successfully</h6>
                            <span id="" class="material-symbols-outlined icon">
                                verified
                            </span>
                        </div>
                    </div><!-- End .form-box -->



                </div><!-- End .container -->
            </div><!-- End .login-page section-bg -->
        </main>

    </section>


    <!-- Plugins JS File -->
    <script src="/assets/js/api/resetPassword.js"></script>
    <script src="/assets/js/jquery.min.js"></script>
    <script src="/assets/js/bootstrap.bundle.min.js"></script>
    <script src="/assets/js/jquery.hoverIntent.min.js"></script>
    <script src="/assets/js/jquery.waypoints.min.js"></script>
    <script src="/assets/js/superfish.min.js"></script>
    <script src="/assets/js/owl.carousel.min.js"></script>
    <!-- Main JS File -->
    <script src="/assets/js/main.js"></script>
</body>


<!-- molla/login.html  22 Nov 2019 10:04:03 GMT -->

</html>